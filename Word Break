 public boolean wordBreak(String s, List<String> wordDict) {
        int n = s.length();
        boolean[] dp = new boolean[n];
        HashSet<String> set = new HashSet<>(wordDict);
        for(int i=0;i<n;++i){
            for(int j=0;j<=i;++j){
                String sub = s.substring(j,i+1);
                if(set.contains(sub) && (j==0 || dp[j-1])){
                    dp[i] = true;
                    break;
                }
            }
        }
        return dp[n-1];
    }
